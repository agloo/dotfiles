Description="eduroam network"
Interface=YOUR INTERFACE
Connection=wireless
Security='wpa-configsection'
IP=dhcp
TimeoutWPA=30
WPAConfigSection=(
    'ssid="AirBears2"'
    'key_mgmt=WPA-EAP'
    'eap=TTLS'
    'proto=WPA2'
    'phase2="auth=PAP"'
    'anonymous_identity="MY CALNET ID"' # ex: tu-dresden.de
    'identity="MY CALNET ID"' # ex: s4234381@tu-dresden.de
    'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.crt"'
    'password="MY PASSWORD"'
)

